{
  "expo": {
    "name": "Love Library",
    "slug": "love-library",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#E91E63",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lovelibrary.app"
    },
    "android": {
      "package": "com.lovelibrary.app",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app needs access to your photos to upload them to your library."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "63dba2fb-627a-4452-8d0c-91ce8422de1a"
      }
    }
  }
}
